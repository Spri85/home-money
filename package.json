  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node serve.js",
    "postinstall": "ng build --prod",
    "build-prod": "ng build --prod",
    "deploy:gh": "concurrently --kill-others \"ng build --prod --base-href 'https://spri85.github.io/home-money/'\" \"ngh\"",
    "deploy:firebase": "concurrently --kill-others \"ng build --prod\" \"firebase deploy\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "json-server --watch db.json --port 3000",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
